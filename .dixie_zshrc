alias slavedb="psql -U czoppa -h slavedb.quickquid.co.uk -p 5432"
alias pbcopy='xsel --clipboard --input'
alias pbpaste='xsel --clipboard --output'

alias git_today='ssh wm "cd /export/comp/stable && git log --all --committer=czoppa --since=24.hours" | pbcopy'
alias git_this_week='ssh wm "cd /export/comp/stable && git log --all --committer=czoppa --since=7.days" | pbcopy'
